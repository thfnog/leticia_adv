.btQuoteBooking .btTotalNextWrapper {
	display: block;
	width: 100%;
	padding-right: 0;
    -webkit-transition: all 400ms ease;
    transition: all 400ms ease;
}
.btQuoteBooking .btTotalNextWrapper:after {
	content: " ";
	display: block;
	width: 100%;
	clear: both;
}
