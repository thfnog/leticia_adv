	clear: both;
	width: 100%;
}
.btQuoteBooking .btQuoteBookingForm {
	margin: 0 0 30px;
}
.btQuoteBooking .btTotalQuoteContactGroup {
    -webkit-transition: all 600ms ease;
    transition: all 600ms ease;
	max-height: 0;
	overflow: hidden;
	opacity: 0;
